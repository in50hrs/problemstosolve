<!DOCTYPE html>
<html>
<%= render 'layouts/head' %>
<body>
<div class="container-narrow">
	<% if notice %>
	<div class="alert">
	  <button type="button" class="close" data-dismiss="alert">&times;</button>
	  <%= notice %>
	</div>
	<%end%>
<%= yield %>
<%= render 'layouts/footer' %>
<%= javascript_include_tag  "jquery", "bootstrap.min" %>
</div>
</body>
</html>
